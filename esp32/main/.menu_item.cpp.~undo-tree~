([nil nil ((2640 . 2651) (#("         " 0 9 (ws-butler-chg chg fontified t)) . 2640) 2651 (t 25139 16284 846410 112000)) nil (25139 16320 299034 621000) 0 nil])
([nil nil ((1379 . 1380)) nil (25139 16320 299007 120000) 0 nil])
([nil nil ((1382 . 1386) (#("           " 0 11 (ws-butler-chg chg fontified t)) . 1382) (t 25139 16320 303075 437000)) nil (25139 16346 194916 649000) 0 nil])
([nil nil ((1400 . 2520) (#("       on_modified();
           break;

         case MenuEvent::Left:
             auto fp_scale = (int)pow(10, precision);
             setter((float)(floor(m_getter() * fp_scale + 0.1f) - step * Menu::modification_speed) / fp_scale);
             render();
             on_modified();
             break;
           case MenuEvent::Reset:
               break;
             default:
                 break;
        }
}

void FloatItem::set_precision(int p) {
  assert (digits < ARRAY_COUNT(DEFAULT_FORMATS));
  fmt = DEFAULT_FORMATS[digits];
  precision = p;
}

// =============================================================================================
// Integer
// =============================================================================================

void IntItem::render() {
  auto val = get();
  char buf[32];
  auto len = snprintf(buf, sizeof(buf), fmt.c_str(), val);
  value(buf);
}

void IntItem::on_event(MenuEvent evt) {
  switch (evt) {
  case MenuEvent::render:
    render();
    break;

  case MenuEvent::PressQuad:
    Menu::ToggleEditMode();
    break;

  case MenuEvent::Right:
    setter(m_getter() + step * Menu::modification_speed;
           render();
