// =============================================================================================
// Floating point
// =============================================================================================

const char FloatItem::DEFAULT_FORMAT[] = \"%.2f\";
const char FloatItem::DEFAULT_FORMATS[7][16] = {\"%.0f\", \"%.1f\", \"%.2f\", \"%.3f\", \"%.4f\", \"%.5f\", \"%.6f\"};

void FloatItem::render() {
    auto val = get();
    char buf[32];
    auto len = snprintf(buf, sizeof(buf), fmt.c_str(), val);
    value(buf);
}

void FloatItem::on_event(MenuEvent evt) {
    switch (evt) {
        case MenuEvent::render:
            render();
            break;
        case MenuEvent::PressQuad:
            Menu::ToggleEditMode();
            break;
        case MenuEvent::Right:
            auto fp_scale = (int)pow(10, precision);
            setter((float)(floor(m_getter() * fp_scale + 0.1f) + step * Menu::modification_speed) / fp_scale;
            render();
            on_modified();
            break;
        case MenuEvent::Left:
            auto fp_scale = (int)pow(10, precision);
            setter((float)(floor(m_getter() * fp_scale + 0.1f) - step * Menu::modification_speed) / fp_scale);
            render();
            on_modified();
            break;
        case MenuEvent::Reset:
            break;
        default:
            break;
    }
}

void FloatItem::set_precision(int p)
{
    assert (digits<ARRAY_COUNT(DEFAULT_FORMATS));
    fmt = DEFAULT_FORMATS[digits];
    precision = p;
}

// =============================================================================================
// Integer
// =============================================================================================

void IntItem::render() {
    auto val = get();
    char buf[32];
    auto len = snprintf(buf, sizeof(buf), fmt.c_str(), val);
    value(buf);
}

void IntItem::on_event(MenuEvent evt) {
    switch (evt) {
        case MenuEvent::render:
            render();
            break;
        case MenuEvent::PressQuad:
            Menu::ToggleEditMode();
            break;
        case MenuEvent::Right:
            setter(m_getter() + step * Menu::modification_speed;
            render();
            on_modified();
            break;
        case MenuEvent::Left:
            setter(m_getter() - step * Menu::modification_speed;
            render();
            on_modified();
            break;
        case MenuEvent::Reset:
            break;
        default:
            break;
    }
}

// =============================================================================================
// Boolean
// =============================================================================================

void BoolItem::render() {
    auto val = get();
    char buf[32];
    auto len = snprintf(buf, sizeof(buf), fmt.c_str(), val ? \"Y\" : \"N\");
    value(buf);
}

void BoolItem::on_event(MenuEvent evt) {
    switch (evt) {
        case MenuEvent::render:
            render();
            break;
        case MenuEvent::PressQuad:
            Menu::ToggleEditMode();
            break;
        case MenuEvent::Right:
            setter(true);
            render();
            on_modified();
            break;
        case MenuEvent::Left:
            setter(false);
            render();
            on_modified();
            break;
        case MenuEvent::Reset:
            break;
        default:
            break;
    }
}

// =============================================================================================
// Action
// =============================================================================================


void ActionItem::render() {
    auto val = get();
    char buf[32];
    auto len = snprintf(buf, sizeof(buf), \"<F>\");
    value(buf);
}

void ActionItem::on_event(MenuEvent evt) {
    switch (evt) {
        case MenuEvent::render:
            render();
            break;
        case MenuEvent::PressQuad:
            action(this, envt);
            break;
        case MenuEvent::Right:
            break;
        case MenuEvent::Left:
            break;
        case MenuEvent::Reset:
            break;
        default:
            break;
